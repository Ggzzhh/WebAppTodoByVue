{
  "name": "vue_todo",
  "version": "1.0.0",
  "description": "学习vue框架",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js --color",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.config.client.js --color",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.config.server.js --color",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "clean": "rimraf public && rimraf server-build",
    "dev:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "cross-env NODE_ENV=production node server/server.js",
    "practice": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.practice.js --color"
  },
